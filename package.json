{
  "name": "jackmoody11.github.io",
  "version": "0.1.0",
  "description": "Jack Moody's Personal Website",
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "env-cmd -f .env gatsby develop",
    "deploy": "gatsby build && gh-pages -d public -b master -r https://$ACCESS_TOKEN@github.com/jackmoody11/jackmoody11.github.io.git",
    "deploy_locally": "gatsby build && gh-pages -d public -b master git+https://github.com/jackmoody11/jackmoody11.github.io.git",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "lint": "eslint . --ignore-path .gitignore --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackmoody11/jackmoody11.github.io.git"
  },
  "keywords": [
    "website"
  ],
  "author": "Jack Moody",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackmoody11/jackmoody11.github.io/issues"
  },
  "homepage": "https://github.com/jackmoody11/jackmoody11.github.io#readme",
  "dependencies": {
    "@narative/gatsby-theme-novela": "0.17.0",
    "gatsby": "^2.32.13",
    "gatsby-plugin-mdx": "^1.1.10",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-plugin-typescript": "^2.3.3",
    "gatsby-remark-images": "^3.2.5",
    "gatsby-remark-relative-images": "^0.3.0",
    "katex": "^0.11.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^13.13.2",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.34",
    "@types/react-syntax-highlighter": "^11.0.4",
    "env-cmd": "^10.1.0",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "typescript": "^3.8.3"
  }
}
